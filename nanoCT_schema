 <xsd:complexType name="eBeamDecelerationDetails">
  <xsd:annotation>
   <xsd:documentation xml:lang="en">
				**********************eBeamDecelerationDetails**********************
				Details about deceleration of the e- beam.

				children:

				eBeamDecelerationOn (required) - Is deceleration applied to the e-beam? true-false, datatype boolean

				landingEnergy (optional) - Landing energy of the e-beam expressed as the complex type energyDetails, allowed units - meV, eV, keV, nJ, mJ, J, kJ, MJ

				stageBias (optional) - Bias voltage applied to the stage expressed as the complex type biasDetails, allowed units - nV, uV, mV, V, kV, nA, uA, mA, A, kA
			</xsd:documentation>
  </xsd:annotation>
  <xsd:all>
   <xsd:element name="landingEnergy" type="energyDetails" minOccurs="0"/>
   <xsd:element name="stageBias" type="voltageDetails" minOccurs="0"/>
  </xsd:all>
 </xsd:complexType>
 <xsd:complexType name="energyDetails">
  <xsd:annotation>
   <xsd:documentation xml:lang="en">
				*************************energyDetails*************************
				complex type describing an energy

				children:

				value (required) - The energy expressed as an integer or floating point value, datatype float

				unit (required) - the unit of the value to be selected from a controlled list meV, eV, keV, MeV, nJ, mJ, J, kJ, MJ default value - keV, datatype string

				qualifier (optional) - A qualifier to describe the value (e.g., avg, max, min), default value - max, datatype string

				uncertainty (optional) - Uncertainty in the value defined as a complex type uncertaintyType, which gives the option of choosing between an absolute uncertainty or relative uncertainty and entering the value of uncertainty as a double value.

				notes (optional) - Further notes about the value, datatype string
			</xsd:documentation>
  </xsd:annotation>
  <xsd:all>
   <xsd:element name="value" type="xsd:float"/>
   <xsd:element name="unit" type="energyUnitType" default="keV"/>
   <xsd:element name="qualifier" type="xsd:string" minOccurs="0" default="max"/>
   <xsd:element name="uncertainty" type="uncertaintyType" minOccurs="0"/>
   <xsd:element name="notes" type="xsd:string" minOccurs="0"/>
  </xsd:all>
 </xsd:complexType>
 <xsd:complexType name="motorValueDetails">
  <xsd:annotation>
   <xsd:documentation xml:lang="en">
				*************************motorValueDetails*************************
				complex type describing an energy

				children:

				value (required) - The energy expressed as an integer or floating point value, datatype float

				unit (required) - the unit of the value to be selected from a controlled list meV, eV, keV, MeV, nJ, mJ, J, kJ, MJ default value - keV, datatype string

				qualifier (optional) - A qualifier to describe the value (e.g., avg, max, min), default value - max, datatype string

				uncertainty (optional) - Uncertainty in the value defined as a complex type uncertaintyType, which gives the option of choosing between an absolute uncertainty or relative uncertainty and entering the value of uncertainty as a double value.

				notes (optional) - Further notes about the value, datatype string
			</xsd:documentation>
  </xsd:annotation>
  <xsd:all>
   <xsd:element name="value" type="xsd:float"/>
   <xsd:element name="unit" type="distanceUnitType"/>
   <xsd:element name="uncertainty" type="uncertaintyType" minOccurs="0"/>
   <xsd:element name="notes" type="xsd:string" minOccurs="0"/>
  </xsd:all>
 </xsd:complexType>
 <xsd:simpleType name="energyUnitType">
  <xsd:restriction base="xsd:string">
   <xsd:enumeration value="meV"/>
   <xsd:enumeration value="eV"/>
   <xsd:enumeration value="keV"/>
   <xsd:enumeration value="MeV"/>
   <xsd:enumeration value="nJ"/>
   <xsd:enumeration value="mJ"/>
   <xsd:enumeration value="J"/>
   <xsd:enumeration value="kJ"/>
   <xsd:enumeration value="MJ"/>
  </xsd:restriction>
 </xsd:simpleType>
 <xsd:complexType name="FIBDetails">
  <xsd:annotation>
   <xsd:documentation xml:lang="en">
				*********************FIBDetails*********************
				Details about the focussed ion beam (FIB) extension if present

				children:

				iBeamSource (required) - describes the details of the FIB source, defined as the complex type fibSourceDetails. It is important to note the accelerating voltage and beam current of the i-beam.

				gunPressure (required) - pressure of the FIB gun, expressed as the complex type pressureDetails, Allowed units Pa, hPa, kPa, MPa, GPa, mbar, bar, psi

				gasInjectionSystem (optional) - Details about the gas injection system (GIS) expressed as GISDetails.

				iBeamWorkingDistance (optional) - Working distance of the ion beam (i-beam), expressed as complex type distanceDetails. This value needs to be entered if the i-beam focus has to be set independently from the e-beam focus. Allowed units: nm, um, mm, cm, m

				FIBSpotSize (optional) - spot size of the i-beam at the focus, when the sample is in focus, this will be also the spot size on the sample, expressed as the complexType distanceDetails, Allowed units: nm, um, mm, cm, m

				angleToEBeam (optional) - angle between e-beam and i-beam expressed as the complex type angleDetails, Allowed units: degree, radian
			</xsd:documentation>
  </xsd:annotation>
  <xsd:all>
   <xsd:element name="iBeamSource" type="fibSourceDetails"/>
   <xsd:element name="gunPressure" type="pressureDetails"/>
   <xsd:element name="gasInjectionSystem" type="GISDetails" minOccurs="0"/>
   <xsd:element name="iBeamWorkingDistance" type="distanceDetails" minOccurs="0"/>
   <xsd:element name="FIBSpotSize" type="distanceDetails" minOccurs="0"/>
   <xsd:element name="angleToEBeam" type="angleDetails" minOccurs="0"/>
  </xsd:all>
 </xsd:complexType>
 <xsd:simpleType name="pressureUnitType">
  <xsd:restriction base="xsd:string">
   <xsd:enumeration value="Pa"/>
   <xsd:enumeration value="hPa"/>
   <xsd:enumeration value="kPa"/>
   <xsd:enumeration value="MPa"/>
   <xsd:enumeration value="GPa"/>
   <xsd:enumeration value="mbar"/>
   <xsd:enumeration value="bar"/>
   <xsd:enumeration value="psi"/>
  </xsd:restriction>
 </xsd:simpleType>
 <xsd:complexType name="GISDetails">
  <xsd:annotation>
   <xsd:documentation>
				**********************GISDetails**********************
				gives the details of the gas injection system (GIS)

				children:

				GISName (optional) - Name of the GIS system, datatype string

				beamDepositionType (optional) - type of beam deposition, datatype string, choice between e-beam deposition and i-beam deposition

				depositionCurrent (optional) - the current used for performing beam deposition, expressed as the complex type currentDetails, Allowed units pA,nA,uA,mA,A

				depositionSize (optional) - the size of the deposition, expressed as the complex type distanceDetails. Allowed units nm,um,mm,cm,m

				depositionTime (optional) - total time for deposition, expressed as the complex type timeDetails. Allowed units ps,ns,us,ms,s
			</xsd:documentation>
  </xsd:annotation>
  <xsd:all>
   <xsd:element name="GISName" type="xsd:string" minOccurs="0"/>
   <xsd:element name="beamDepositionType" type="depositionType" minOccurs="0"/>
   <xsd:element name="depositionCurrent" type="currentDetails" minOccurs="0"/>
   <xsd:element name="depositionSize" type="distanceDetails" minOccurs="0"/>
   <xsd:element name="depositionTime" type="timeDetails" minOccurs="0"/>
  </xsd:all>
 </xsd:complexType>
 <xsd:simpleType name="depositionType">
  <xsd:restriction base="xsd:string">
   <xsd:enumeration value="E-beam Deposition"/>
   <xsd:enumeration value="I-Beam Deposition"/>
  </xsd:restriction>
 </xsd:simpleType>
 <xsd:simpleType name="scanType">
  <xsd:restriction base="xsd:string">
   <xsd:enumeration value="fly scan"/>
   <xsd:enumeration value="step scan"/>
  </xsd:restriction>
 </xsd:simpleType>
