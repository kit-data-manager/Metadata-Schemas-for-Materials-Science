 <xsd:complexType name="detectorDetails">
  <xsd:annotation>
   <xsd:documentation xml:lang="en">
				*************************detectorDetails*************************
				gives the settings of a detector.

				children:

				detectorType (required) - The type of detector (e.g., surface electron detection,  secondary electron, back-scattered electron), datatype string.

				detectorName (required) -Name of the detector, datatype string.

				detectorID (optional) - Identifier for the detector expressed as the complex type identifier.

				detectorManufacturer (optional) - describes the details of the detector like manufacturerName, modelName and detectorID as part of the complex type manufacturerDetails.

				componentGeometry (optional) - xyz coordinates describing the position of the detector expressed as the complex group coordinateSet.

				lastCalibration (optional) - date and time of last calibration expressed as CCYY-MM-DDThh:mm:ss.sss conforming to xsd:dateTime.

				detectorBias (optional) - bias voltage applied to the detector expressed as complex type voltageDetails. Allowed units nV, uV, mV, V, kV.
			</xsd:documentation>
  </xsd:annotation>
  <xsd:all>
   <xsd:element name="detectorType" type="detectorType" minOccurs="1" maxOccurs="1"/>
   <xsd:element name="detectorName" type="xsd:string"/>
   <xsd:element name="detectorID" type="identifier" minOccurs="0"/>
   <xsd:element name="detectorManufacturer" type="manufacturerDetails" minOccurs="0"/>
   <xsd:element name="detectorPosition" type="coordinateSet" minOccurs="0"/>
   <xsd:element name="lastCalibration" type="xsd:dateTime" minOccurs="0"/>
   <xsd:element name="detectorBias" type="voltageDetails" minOccurs="0"/>
   <xsd:element name="nativePixelSize" type="pixelSize3D"/>
   <xsd:element name="numberOfPixels" type="pixelNumberType"/>
   <xsd:element name="sensor" type="sensorDetails"/>
   <xsd:element name="lens" type="lensDetails"/>
   <xsd:element name="bitDepth" type="xsd:string"/>
  </xsd:all>
 </xsd:complexType>
